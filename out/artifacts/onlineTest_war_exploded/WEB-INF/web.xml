<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>/WEB-INF/jsp/index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>getQuestions</servlet-name>
        <servlet-class>com.juhick.onlineTest.getQuestions</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getQuestions</servlet-name>
        <url-pattern>/onlineTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getScore</servlet-name>
        <servlet-class>com.juhick.onlineTest.getScore</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getScore</servlet-name>
        <url-pattern>/Result</url-pattern>
    </servlet-mapping>
</web-app>